{
  "name": "enupal/stripe",
  "description": "Craft Pay allows customers sign up for recurring and one-time payments with Stripe, perfect for orders, donations, subscriptions, and events. Create simple payment forms in seconds easily without coding. For Craft CMS 3.x",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "stripe",
    "buy now",
    "sell",
    "payment",
    "credit card",
    "pay",
    "checkout",
    "e-commerce"
  ],
  "autoload": {
    "psr-4": {
      "enupal\\stripe\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Enupal",
      "homepage": "http://enupal.com/en"
    }
  ],
  "support": {
    "email": "support@enupal.com",
    "issues": "https://github.com/enupal/stripe/issues",
    "docs": "https://enupal.com/en/craft-plugins/enupal-stripe/docs"
  },
  "require": {
    "craftcms/cms": "^3.0.0",
    "stripe/stripe-php": "^6.6.0"
  },
  "extra": {
    "name": "Craft Pay",
    "handle": "enupal-stripe",
    "schemaVersion": "1.0.0",
    "hasSettings": true,
    "changelogUrl": "https://raw.githubusercontent.com/enupal/stripe/master/CHANGELOG.md",
    "developer": "Enupal",
    "class": "enupal\\stripe\\Stripe",
    "components": {
      "app": "enupal\\stripe\\services\\App"
    }
  }
}
